 * currentTime1/duration;\r\n\t\t\t\t\t//that.playSetFooterData({percentage:percentage});\r\n\t\t\t\t\tthat.footerData.audioData.percentage=percentage;\r\n\t\t\t\t});\r\n\t\t\t\t//播放完成\r\n\t\t\t\taudio.addEventListener('ended',function(e){\r\n\t\t\t\t\t//that.playSetFooterData({icon:\"icon-bofang\"});\r\n\t\t\t\t\tthat.footerData.audioData.icon=\"icon-bofang\";\r\n\t\t\t\t\tfor(var i=0;i<that.$store.state.advantage.length;i++){\r\n\t\t\t\t\t\tthat.$store.state.advantage[i].click='';\r\n\t\t\t\t\t}\r\n\t\t\t\t\tthat.$store.state.advantage[idx].hover='hover';\r\n\t\t\t\t\tif(idx<5){\r\n\t\t\t\t\t\tthat.$store.state.advantage[idx+1].click='on';\r\n\t\t\t\t\t}else{\r\n\t\t\t\t\t\tthat.jx='on';\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/chanpin"}]}