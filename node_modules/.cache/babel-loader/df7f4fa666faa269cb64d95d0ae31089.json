A;AACA,IAAA,UAjCA,sBAiCA,CAjCA,EAiCA;AACA,UAAA,IAAA,GAAA,IAAA;AAAA,UAAA,IAAA,GAAA,IAAA,CAAA,UAAA,CAAA,IAAA;AAAA,UAAA,IAAA,GAAA,IAAA,CAAA,UAAA,CAAA,IAAA;;AACA,UAAA,IAAA,EAAA;AACA,YAAA,OAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA,EAAA,OAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,OAAA;;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,OAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,OAAA,CAAA,CAAA,CAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,OAAA,CAAA,CAAA,GAAA,CAAA,CAAA,CAAA,IAAA;;AACA,gBAAA,OAAA,CAAA,CAAA,GAAA,CAAA,CAAA,EAAA;AACA,cAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,gBAAA,IAAA,EAAA,OAAA,CAAA,CAAA,GAAA,CAAA,CAAA,CAAA;AAAA,eAAA,EADA,CAEA;AACA;AACA;AACA;AACA,OAZA,MAYA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AACA,OAhBA,CAiBA;AACA;;AACA,KApDA;AAqDA;AACA,IAAA,WAtDA,uBAsDA,IAtDA,EAsDA,KAtDA,EAsDA;AACA,UAAA,IAAA,GAAA,IAAA;AAAA,UAAA,QAAA,GAAA,CAAA,MAAA,CAAA,IAAA,CAAA,GAAA,MAAA,CAAA,KAAA,CAAA,GAAA,GAAA,EAAA,OAAA,CAAA,CAAA,CAAA;AACA,MAAA,IAAA,CAAA,QAAA,GAAA,QAAA;AACA;AAzDA;AA9CA,CAAA","sourcesContent":["<template>\r\n<div class=\"lh-footer\">\r\n    <div class=\"menu left\" :class=\"menuShow\">\r\n        <em @click=\"openMenu($event)\"><i class=\"iconfont icon-menu\"></i><span class=\"title\">菜单</span></em>\r\n        <div class=\"menu-layer\">\r\n            <h3 class=\"title\">目录<i class=\"iconfont icon-l-close\" @click=\"closeMenu($event)\"></i></h3>\r\n            <dl>\r\n                <dt v-for=\"item in type[footerData.type].directory\" :data-path=\"item.path\" @click=\"goPage($event)\">{{item.name}}</dt>\r\n                <!-- <dd>起源与历史</dd>\r\n                <dd>独特性</dd> -->\r\n            </dl>\r\n        </div>\r\n    </div>\r\n\t<div class=\"home left\"><router-link to=\"/\"><i class=\"iconfont icon-shouye\"></i>首页</router-link></div>\r\n    <div class=\"course\">\r\n        <span class=\"left title\">课程进度</span>\r\n        <div class=\"course-progress left\">\r\n            <div class=\"hd\" :style=\"{width:''+progress+'%'}\"></div>\r\n            <em v-if=\"progress>0\">{{progress}}%</em>\r\n        </div>\r\n        <div class=\"pages clearfix left\">第{{footerData.page}}/{{type[footerData.type].pages}}页</div>\r\n    </div>\r\n    <div class=\"lh-music left clearfix\">\r\n    \t<div class=\"btn left\" @click=\"play($event)\"><i class=\"iconfont\" :class=\"[footerData.audioData.icon]\"></i></div>\r\n        <div class=\"current left\">{{footerData.audioData.current}}</div>\r\n        <div class=\"progress left\">\r\n        \t<div class=\"hd\" :style=\"{width:''+footerData.audioData.percentage+'%'}\"></div>\r\n        </div>\r\n        <div class=\"total left\">{{footerData.audioData.total}}</div>\r\n    </div>\r\n    <div class=\"lh-text left\">\r\n    \t<div class=\"text\">音频文字</div>\r\n        <div class=\"lh-text-layer\" v-if=\"footerData.audioData.text\">\r\n            {{footerData.audioData.text}}\r\n        </div>\r\n    </div>\r\n    <div class=\"lh-share left\">\r\n        <input name=\"text\" type=\"text\" />\r\n        <i class=\"iconfont icon-fasong\"></i>\r\n    </div>\r\n    <!-- <div class=\"lh-btn clearfix left\">\r\n        <span :data-page=\"footerData.page\" :data-type=\"footerData.type\" @click=\"routerback($event)\"><i class=\"iconfont icon-l-xiangzuo\"></i>返回</span>\r\n        <span>下一页<i class=\"iconfont icon-l-xiangyou\"></i></span>\r\n    </div> -->\r\n    <audio id=\"audio\" src=\"\"></audio>\r\n</div>\r\n</template>\r\n<script>\r\n//import {mapState} from 'vuex'\r\nexport default {\r\n    data(){\r\n        return {\r\n            menuShow:'',\r\n            type:{\r\n                culture:{//酒文化\r\n                    pages:12,\r\n                    directory:[\r\n                        {name:'1909',path:'history1909'},\r\n                        {name:'1932',path:'history?year=1932&page=2'},\r\n                        {name:'1950',path:'history?year=1950&page=3'},\r\n                        {name:'1952',path:'history?year=1952&page=4'},\r\n                        {name:'1959',path:'history?year=1959&page=5'},\r\n                        {name:'探索',path:'explore'},\r\n                        {name:'1960',path:'history?year=1960&page=6'},\r\n                        {name:'1978',path:'history?year=1978&page=7'},\r\n                        {name:'1985',path:'history?year=1985&page=8'},\r\n                        {name:'1992',path:'history?year=1992&page=9'},\r\n                        {name:'1995',path:'history?year=1995&page=10'},\r\n                        {name:'1998',path:'history?year=1998&page=11'},\r\n                        {name:'2017',path:'history?year=2017&page=12'},\r\n                    ]\r\n                }\r\n            }\r\n        }\r\n    },\r\n    computed:{\r\n        //...mapState(['footerData','name'])\r\n    },\r\n    created(e){\r\n        var that=this;\r\n        var page=that.footerData.page,typeName=that.footerData.type,pages=that.type[typeName].pages;\r\n        //课程进度\r\n        that.progressCal(page,pages);\r\n    },\r\n    beforeUpdate(e){\r\n        var that=this;\r\n        var page=that.footerData.page,typeName=that.footerData.type,pages=that.type[typeName].pages;\r\n        //课程进度\r\n        that.progressCal(page,pages);\r\n    },\r\n    props:{\r\n        footerData:{\r\n            type:Object\r\n        }\r\n    },\r\n    methods:{\r\n        play(e){\r\n            var that=this;\r\n            var audio = $('#audio')[0];\r\n            if($('#audio').attr('src')){\r\n                if(audio.paused){\r\n                    audio.play();\r\n                    that.footerData.audioData.icon=\"icon-zanting\";   \r\n                }else{\r\n                    audio.pause();\r\n                    that.footerData.audioData.icon=\"icon-bofang\";    \r\n                }\r\n            }\r\n        },\r\n        goPage(e){\r\n            var that=this,path = $(e.currentTarget).data('path');\r\n            console.log(path);\r\n            if(path==this.$route.name){\r\n                this.$router.go(0);\r\n            }else{\r\n                this.$router.push({ path: path});\r\n                this.$router.go(0);\r\n            }\r\n        },\r\n        openMenu(e){\r\n            var that=this;\r\n            that.menuShow='on';\r\n        },\r\n        closeMenu(e){\r\n            var that=this;\r\n            that.menuShow='';\r\n        },\r\n        //返回路由\r\n        routerback(e){\r\n            var that=this,page = that.footerData.page,type = that.footerData.type;\r\n            if(page){\r\n                var routers=that.type[type].routers;\r\n                console.log(routers);\r\n                for(var i=0;i<routers.length;i++){\r\n                    if(routers[i].page===page){\r\n                        console.log(routers[i-1].path);\r\n                        if(routers[i-1]){\r\n                            that.$router.push({ path: routers[i-1].path});\r\n                            //that.$router.go(0);\r\n                        }\r\n                    }\r\n                }\r\n            }else{\r\n               this.$router.back(-1); \r\n            }\r\n            //console.log(this.$route.name);\r\n            //this.$router.back(-1);\r\n        },\r\n        //课程进度计算\r\n        progressCal(page,pages){\r\n            var that=this, progress=(Number(page)/Number(pages)*100).toFixed(0);\r\n            that.progress=progress;\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/components"}]}