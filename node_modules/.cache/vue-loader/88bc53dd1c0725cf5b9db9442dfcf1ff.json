nt":["<template>\r\n<div class=\"lh-footer\">\r\n    <div class=\"menu left\" :class=\"menuShow\">\r\n        <em @click=\"openMenu($event)\"><i class=\"iconfont icon-menu\"></i><span class=\"title\">菜单</span></em>\r\n        <div class=\"menu-layer\">\r\n            <h3 class=\"title\">目录<i class=\"iconfont icon-l-close\" @click=\"closeMenu($event)\"></i></h3>\r\n            <dl>\r\n                <dt v-for=\"item in type[footerData.type].directory\" :data-path=\"item.path\" @click=\"goPage($event)\">{{item.name}}</dt>\r\n                <!-- <dd>起源与历史</dd>\r\n                <dd>独特性</dd> -->\r\n            </dl>\r\n        </div>\r\n    </div>\r\n\t<div class=\"home left\"><router-link to=\"/\" replace><i class=\"iconfont icon-shouye\"></i>首页</router-link></div>\r\n    <div class=\"course\">\r\n        <span class=\"left title\">课程进度</span>\r\n        <div class=\"course-progress left\">\r\n            <div class=\"hd\" :style=\"{width:''+progress+'%'}\"></div>\r\n            <em v-if=\"progress>0\">{{progress}}%</em>\r\n        </div>\r\n        <div class=\"pages clearfix left\">第{{footerData.page}}/{{type[footerData.type].pages}}页</div>\r\n    </div>\r\n    <div class=\"lh-music left clearfix\">\r\n    \t<div class=\"btn left\" @click=\"play($event)\"><i class=\"iconfont\" :class=\"[footerData.audioData.icon]\"></i></div>\r\n        <div class=\"current left\">{{footerData.audioData.current}}</div>\r\n        <div class=\"progress left\">\r\n        \t<div class=\"hd\" :style=\"{width:''+footerData.audioData.percentage+'%'}\"></div>\r\n        </div>\r\n        <div class=\"total left\">{{footerData.audioData.total}}</div>\r\n    </div>\r\n    <div class=\"lh-text left\">\r\n    \t<div class=\"text\" :class=\"textLayerOn\" @click=\"opentextLayer($event)\">音频文字</div>\r\n        <div class=\"lh-text-layer\" v-if=\"footerData.audioData.text\">\r\n            {{footerData.audioData.text}}\r\n        </div>\r\n    </div>\r\n    <div class=\"lh-share left\">\r\n        <input name=\"text\" type=\"text\" />\r\n        <i class=\"iconfont icon-fasong\"></i>\r\n    </div>\r\n    <!-- <div class=\"lh-btn clearfix left\">\r\n        <span :data-page=\"footerData.page\" :data-type=\"footerData.type\" @click=\"routerback($event)\"><i class=\"iconfont icon-l-xiangzuo\"></i>返回</span>\r\n        <span>下一页<i class=\"iconfont icon-l-xiangyou\"></i></span>\r\n    </div> -->\r\n    <audio id=\"audio\" src=\"\"></audio>\r\n</div>\r\n</template>\r\n<script>\r\n//import {mapState} from 'vuex'\r\nexport default {\r\n    inject: ['reload'],\r\n    data(){\r\n        return {\r\n            menuShow:'',\r\n            textLayerOn:'',\r\n            type:{\r\n                culture:{//酒文化\r\n                    pages:12,\r\n                    directory:[\r\n                        {name:'1909',path:'history1909'},\r\n                        {name:'1932',path:'history?year=1932&page=2'},\r\n                        {name:'1950',path:'history?year=1950&page=3'},\r\n                        {name:'1952',path:'history?year=1952&page=4'},\r\n                        {name:'1959',path:'history?year=1959&page=5'},\r\n                        {name:'探索',path:'explore'},\r\n                        {name:'1960',path:'history?year=1960&page=6'},\r\n                        {name:'1978',path:'history?year=1978&page=7'},\r\n                        {name:'1985',path:'history?year=1985&page=8'},\r\n                        {name:'1992',path:'history?year=1992&page=9'},\r\n                        {name:'1995',path:'history?year=1995&page=10'},\r\n                        {name:'1998',path:'history?year=1998&page=11'},\r\n                        {name:'2017',path:'history?year=2017&page=12'},\r\n                    ]\r\n                }\r\n            }\r\n        }\r\n    },\r\n    computed:{\r\n        //...mapState(['footerData','name'])\r\n    },\r\n    created(e){\r\n        var that=this;\r\n        var page=that.footerData.page,typeName=that.footerData.type,pages=that.type[typeName].pages;\r\n        //课程进度\r\n        that.updateProgress();\r\n        that.progressCal(page,pages);\r\n        that.getBarrage();\r\n    },\r\n    beforeUpdate(e){\r\n        var that=this;\r\n        var page=that.footerData.page,typeName=that.footerData.type,pages=that.type[typeName].pages;\r\n        //课程进度\r\n        that.progressCal(page,pages);\r\n    },\r\n    props:{\r\n        footerData:{\r\n            type:Object\r\n        }\r\n    },\r\n    // watch:{\r\n    //     $route(to,from){\r\n    //         this.$router.go(0);\r\n    //       },\r\n    // },\r\n    methods:{\r\n        opentextLayer(e){\r\n            if(this.footerData.audioData.text){\r\n                if(this.textLayerOn){\r\n                    this.textLayerOn=\"\";\r\n                    $('.lh-text-layer').hide();\r\n                }else{\r\n                    this.textLayerOn=\"on\";\r\n                    $('.lh-text-layer').show();\r\n                }\r\n            }\r\n        },\r\n        play(e){\r\n            var that=this;\r\n            var audio = $('#audio')[0];\r\n            if($('#audio').attr('src')){\r\n                if(audio.paused){\r\n                    audio.play();\r\n                    that.footerData.audioData.icon=\"icon-zanting\";   \r\n                }else{\r\n                    audio.pause();\r\n                    that.footerData.audioData.icon=\"icon-bofang\";    \r\n                }\r\n            }\r\n        },\r\n        goPage(e){\r\n            var that=this,path = $(e.currentTarget).data('path');\r\n            //that.paths=path;\r\n            //var path='#/history?year=2017&page=12';\r\n           // this.$router.replace({ path: path});\r\n            //this.$router.go(0);\r\n            window.location.replace('#/'+path);\r\n            this.reload();\r\n            //that.$router.go(0);\r\n            //window.location.href = '#/'+path;\r\n            //this.$router.go(0);\r\n            //location.reload();\r\n        },\r\n        openMenu(e){\r\n            var that=this;\r\n            that.menuShow='on';\r\n        },\r\n        closeMenu(e){\r\n            var that=this;\r\n            that.menuShow='';\r\n        },\r\n        //返回路由\r\n        routerback(e){\r\n            var that=this,page = that.footerData.page,type = that.footerData.type;\r\n            if(page){\r\n                var routers=that.type[type].routers;\r\n                console.log(routers);\r\n                for(var i=0;i<routers.length;i++){\r\n                    if(routers[i].page===page){\r\n                        console.log(routers[i-1].path);\r\n                        if(routers[i-1]){\r\n                            that.$router.replace({ path: routers[i-1].path});\r\n                            //that.$router.go(0);\r\n                        }\r\n                    }\r\n                }\r\n            }else{\r\n               this.$router.back(-1); \r\n            }\r\n            //console.log(this.$route.name);\r\n            //this.$router.back(-1);\r\n        },\r\n        //课程进度计算\r\n        progressCal(page,pages){\r\n            var that=this, progress=(Number(page)/Number(pages)*100).toFixed(0);\r\n            that.progress=progress;\r\n        },\r\n        //更新课件进度\r\n        updateProgress(e){\r\n          var that=this, uuid=localStorage.getItem(\"uuid\"),userid=localStorage.getItem(\"userid\"),address=localStorage.getItem(\"address\")+'?uuid='+uuid+'&userid='+userid;\r\n          var url='http://zhishi.wuliangye.com.cn:8866/wlyApp/updateH5Progres.html';\r\n          var progres=that.footerData.page;\r\n          var data={userid:userid,uuid:uuid,address:address,progres:progres,chapter:progres}\r\n          if(userid&&uuid){ \r\n              $.ajax({\r\n                type:'POST',\r\n                url:url,\r\n                data:data,\r\n                dataType:'json',\r\n                success: function(res){\r\n                    console.log(res);\r\n                }\r\n            }); \r\n          } \r\n        },\r\n        //发送弹幕\r\n        sendBarrage(e){\r\n            var that=this, cont =this.$refs.cont.value,uuid=localStorage.getItem(\"uuid\"),userid=localStorage.getItem(\"userid\");\r\n            var section=that.footerData.page;\r\n            if(cont){\r\n                var url='http://zhishi.wuliangye.com.cn:8866/wlyApp/saveAskanswer.html';\r\n                var data={userid:userid,uuid:uuid,content:cont,sec:'3000',section:section}\r\n                $.ajax({\r\n                    type:'POST',\r\n                    url:url,\r\n                    data:data,\r\n                    dataType:'json',\r\n                    success: function(res){\r\n                        console.log(res);\r\n                        if(res.res===1){\r\n                            alert('发送弹幕成功,正在审核~');\r\n                        }   \r\n                    }\r\n                }); \r\n                // axios.post(url,data).then(function(res){\r\n                //     console.log(res);\r\n                // }).catch(function(error){\r\n                //     console.log(error);\r\n                // });\r\n            }\r\n        },\r\n        //获取弹幕\r\n        getBarrage(e){\r\n            var that=this,url='http://zhishi.wuliangye.com.cn:8866/wlyApp/listAskanswer.html',uuid=localStorage.getItem(\"uuid\");\r\n            var section=that.footerData.page;\r\n            var that=this,data={uuid:'8d865efc-207e-4076-a81e-322817f1e40e'};\r\n            // var looper_time=3*1000,looper;\r\n            // var run_once=true;\r\n            // do_barrager();\r\n            // function do_barrager(){\r\n            //   if(run_once){\r\n            //       //如果是首次执行,则设置一个定时器,并且把首次执行置为false\r\n            //       looper=setInterval(do_barrager,looper_time);                \r\n            //       run_once=false;\r\n            //   }\r\n               \r\n            // }\r\n            if(uuid){    \r\n            $.ajax({\r\n                type:'POST',\r\n                url:url,\r\n                data:data,\r\n                dataType:'json',\r\n                success: function(res){\r\n                    var itemArray=[],looper_time=1000;\r\n                    if(res.res===1){\r\n                        for(var i=0;i<res.rows.length;i++){\r\n                            if(res.rows[i].section==section){\r\n                                res.rows[i].info=res.rows[i].content;\r\n                                res.rows[i].bottom=mathRandom($(window).height()/1.5,$(window).height()-50);\r\n                                res.rows[i].close=false;\r\n                                res.rows[i].href='javascript:;';\r\n                                itemArray.push(res.rows[i]);\r\n                            }\r\n                        }\r\n                        console.log(itemArray.length);\r\n                        if(itemArray.length>0){\r\n                            var looper_time=3*1000,run_once=true,total=itemArray.length,index=0;\r\n                            barrager();\r\n                            function barrager(){\r\n                              if(run_once){\r\n                                  //如果是首次执行,则设置一个定时器,并且把首次执行置为false\r\n                                  that.looper=setInterval(barrager,looper_time);                \r\n                                  run_once=false;\r\n                              }\r\n                              //发布一个弹幕\r\n                              $('body').barrager(itemArray[index]);\r\n                              //索引自增\r\n                              index++;\r\n                              //所有弹幕发布完毕，清除计时器。\r\n                              if(index == total){\r\n                                  clearInterval(that.looper);\r\n                                  return false;\r\n                              }\r\n                            }\r\n                        }else{\r\n                            $.fn.barrager.removeAll();\r\n                        }\r\n                    }\r\n                    \r\n                }\r\n            });\r\n            function mathRandom(min, max) {\r\n              return Math.floor(Math.random() * (max - min)) + min   \r\n            }\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>"]}]}